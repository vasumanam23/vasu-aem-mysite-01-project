<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="mysite01.dialog.editor">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <authorinfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="authorinfo"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <authors
                                        granite:class="cq-dialog-dropdown-showhide"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Authors"
                                        name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <author1
                                                jcr:primaryType="nt:unstructured"
                                                text="Author1"
                                                value="author1"/>
                                            <author2
                                                jcr:primaryType="nt:unstructured"
                                                text="Author2"
                                                value="author2"/>
                                            <author3
                                                jcr:primaryType="nt:unstructured"
                                                text="Author3"
                                                value="author3"/>
                                            <author4
                                                jcr:primaryType="nt:unstructured"
                                                text="Author4"
                                                value="author4"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"/>
                                    </authors>
                                    <author1
                                        granite:class="hide list-option-listfrom-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <clgname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="ClgName"
                                                name="./clgName"/>
                                            <course
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Course"
                                                name="./course"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="author1"/>
                                    </author1>
                                    <author2
                                        granite:class="hide list-option-listfrom-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Author2 Student"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <stdname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="StdName"
                                                name="./stdName"/>
                                            <class
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Class"
                                                name="./class"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="author2"/>
                                    </author2>
                                    <author3
                                        granite:class="hide list-option-listfrom-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Author3 Parents"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <fname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Fathor Name"
                                                name="./fname"/>
                                            <mname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mother Name"
                                                name="./mname"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="author3"/>
                                    </author3>
                                    <author4
                                        granite:class="hide list-option-listfrom-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Author4 Location"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <location
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Location"
                                                name="./location"/>
                                            <area
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Area"
                                                name="./area"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="author4"/>
                                    </author4>
                                </items>
                            </column>
                        </items>
                    </authorinfo>
                    <status
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Status"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <gender
                                granite:class="cq-dialog-dropdown-showhide"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Gender">
                                <items jcr:primaryType="nt:unstructured">
                                    <male
                                        jcr:primaryType="nt:unstructured"
                                        text="Male"
                                        value="male"/>
                                    <female
                                        jcr:primaryType="nt:unstructured"
                                        text="Female"
                                        value="female"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cq-dialog-dropdown-showhide-target=".showhidetargets"/>
                            </gender>
                            <targetm
                                granite:class="showhidetargets"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <mfield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Mr."
                                        name="./mfield"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="male"/>
                            </targetm>
                            <targetf
                                granite:class="showhidetargets"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <ffield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="mrs."
                                        name="./ffield"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="female"/>
                            </targetf>
                        </items>
                    </status>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
